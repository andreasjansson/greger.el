(defun greger--finish-response (agent-state)
  "Finish the agent response using AGENT-STATE."
  (greger--debug "=== FINISHING RESPONSE - CONVERSATION COMPLETE ===")
  (with-current-buffer (greger-state-chat-buffer agent-state)
    (goto-char (point-max))
