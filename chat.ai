(defun greger-lsp-test-requirements-met-p ()
  "Check if requirements for LSP tests are met."
  (and (fboundp 'lsp)
       (fboundp 'python-mode)
       (or (executable-find "pyright")
           (executable-find "pylsp")
           (executable-find "python-lsp-server"))))

