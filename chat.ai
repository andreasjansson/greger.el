(defun greger-parser--tool-params-to-markdown (id input)
  "Convert tool parameters with ID and INPUT to markdown."
  (if (null input)
      ""
    (mapconcat (lambda (param)
                 (let ((name (symbol-name (car param)))
                       (value (cdr param)))
                   (concat "### " name "\n\n"
                           "<tool." id ">\n"
                           (greger-parser--value-to-string value) "\n"
                           "</tool." id ">")))
               input "\n\n")))

